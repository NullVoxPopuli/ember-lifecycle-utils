{
  "name": "ember-lifecycle-utils",
  "version": "1.0.3",
  "description": "helper utils for lifecycle related things",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/NullVoxPopuli/ember-lifecycle-utils",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^5.7.2",
    "ember-cli-typescript": "^4.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^5.0.0",
    "@embroider/test-setup": "^4.0.0",
    "@glimmer/component": "^2.0.0",
    "@glimmer/tracking": "^1.0.4",
    "@nullvoxpopuli/eslint-configs": "^5.0.0",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/ember-qunit": "^6.0.0",
    "@types/ember-resolver": "^9.0.0",
    "@types/ember__application": "^4.0.0",
    "@types/ember__array": "^4.0.0",
    "@types/ember__component": "^4.0.0",
    "@types/ember__controller": "^4.0.0",
    "@types/ember__debug": "^4.0.0",
    "@types/ember__engine": "^4.0.0",
    "@types/ember__error": "^4.0.0",
    "@types/ember__object": "^4.0.0",
    "@types/ember__polyfills": "^4.0.0",
    "@types/ember__routing": "^4.0.0",
    "@types/ember__runloop": "^4.0.0",
    "@types/ember__service": "^4.0.0",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^4.0.0",
    "@types/ember__test": "^4.0.0",
    "@types/ember__test-helpers": "^2.6.1",
    "@types/ember__utils": "^4.0.0",
    "@types/htmlbars-inline-precompile": "^3.0.0",
    "@types/qunit": "^2.11.3",
    "@types/rsvp": "^4.0.4",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.4.3",
    "ember-cli": "~6.6.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^3.0.0",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-modifier": "^4.0.0",
    "ember-page-title": "^9.0.0",
    "ember-qunit": "^9.0.0",
    "ember-resolver": "^13.0.0",
    "ember-source": "~6.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^7.0.0",
    "ember-try": "^4.0.0",
    "eslint": "^9.0.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit": "^2.18.0",
    "qunit-dom": "^3.0.0",
    "semantic-release": "^24.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.70.0"
  },
  "release": {
    "branches": [
      "main",
      "master"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "volta": {
    "node": "14.19.1",
    "yarn": "1.22.22"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
